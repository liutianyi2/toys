        document.getElementById("govKW").onkeydown = function (e) {
            e = e || window.event;
            if (e.keyCode == 13) {
                govSearch();
            }
        };

        function govSearch() {
            var keyword = document.getElementById('govKW').value;
            if (keyword == '' || keyword == "请输入关键词") {
                alert("请输入关键词");
            } else {
                window.location = '/publicity?IsSearch=true&title=' + encodeURI(keyword);
            }
        }

    </script>
